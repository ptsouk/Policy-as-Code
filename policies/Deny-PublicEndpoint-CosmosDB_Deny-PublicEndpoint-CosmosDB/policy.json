{
 "properties": {
  "displayName": "Deny-PublicEndpoint-CosmosDB",
  "policyType": "Custom",
  "mode": "All",
  "description": "This policy restrict creation of cosmos db accounts with exposed public endpoints",
  "metadata": {
   "category": "Deny-PublicEndpoint",
   "createdBy": "1190845d-145d-4f91-ab69-2bc695a48665",
   "createdOn": "2020-06-23T09:14:44.6241317Z",
   "updatedBy": null,
   "updatedOn": null
  },
  "parameters": {},
  "policyRule": {
   "if": {
    "allOf": [
     {
      "field": "type",
      "equals": "Microsoft.DocumentDB/databaseAccounts"
     },
     {
      "field": "Microsoft.DocumentDB/databaseAccounts/publicNetworkAccess",
      "notequals": "Disabled"
     }
    ]
   },
   "then": {
    "effect": "Deny"
   }
  }
 },
 "id": "/providers/Microsoft.Management/managementGroups/a6e09f1d-1f05-497b-b499-da099ced752f/providers/Microsoft.Authorization/policyDefinitions/Deny-PublicEndpoint-CosmosDB",
 "type": "Microsoft.Authorization/policyDefinitions",
 "name": "Deny-PublicEndpoint-CosmosDB"
}