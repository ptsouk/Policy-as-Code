{
 "properties": {
  "displayName": "Deny-PublicEndpoint-Aks",
  "policyType": "Custom",
  "mode": "All",
  "description": "This policy restricts creation of non-private AKS clusters",
  "metadata": {
   "category": "Deny-PublicEndpoint"
  },
  "parameters": {},
  "policyRule": {
   "if": {
    "allOf": [
     {
      "field": "type",
      "equals": "Microsoft.ContainerService/managedClusters"
     },
     {
      "field": "Microsoft.ContainerService/managedClusters/apiServerAccessProfile.enablePrivateCluster",
      "notequals": "true"
     }
    ]
   },
   "then": {
    "effect": "Deny"
   }
  }
 },
 "id": "/providers/Microsoft.Management/managementGroups/ES/providers/Microsoft.Authorization/policyDefinitions/Deny-PublicEndpoint-Aks",
 "type": "Microsoft.Authorization/policyDefinitions",
 "name": "Deny-PublicEndpoint-Aks"
}